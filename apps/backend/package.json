{
  "name": "@caeli/backend",
  "version": "1.0.0",
  "private": true,
  "description": "Fastify backend application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/__tests__/unit",
    "test:integration": "vitest run src/__tests__/integration"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@psteinroe/fastify-supabase": "^0.1.0",
    "@supabase/supabase-js": "^2.75.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "chalk": "4.1.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "fastify-type-provider-zod": "^6.0.0",
    "pino": "^10.0.0",
    "pino-pretty": "^10.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.10",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  }
}
